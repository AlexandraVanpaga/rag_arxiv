    "\n",
    "class ArxivRAG:\n",
    "    \"\"\"RAG система для поиска по статьям arXiv\"\"\"\n",
    "    \n",
    "    def __init__(\n",
    "        self,\n",
    "        embedder_name: str = \"BAAI/bge-base-en-v1.5\",  \n",
    "        reranker_name: str = \"BAAI/bge-reranker-base\",\n",
    "        use_reranker: bool = True\n",
    "    ):\n",
    "        self.model_manager = ModelManager()\n",
    "        self.embedder_name = embedder_name\n",
    "        self.reranker_name = reranker_name\n",
    "        self.use_reranker = use_reranker\n",
    "        \n",
    "        # Ленивая загрузка моделей\n",
    "        self.embedder = None\n",
    "        self.reranker = None\n",
    "        \n",
    "        # Хранилище данных\n",
    "        self.index = None\n",
    "        self.metadata_df = None\n",
    "        self.embeddings = None\n",
    "        \n",
    "    def _get_embedder(self):\n",
    "        if self.embedder is None:\n",
    "            self.embedder = self.model_manager.get_embedder(self.embedder_name)\n",
    "        return self.embedder\n",
    "    \n",
    "    def _get_reranker(self):\n",
    "        if self.reranker is None and self.use_reranker:\n",
    "            self.reranker = self.model_manager.get_reranker(self.reranker_name)\n",
    "        return self.reranker\n",
    "    \n",
    "    def load_data(self, metadata_path: Optional[str] = None) -> pd.DataFrame:\n",
    "        \"\"\"Загрузка данных arXiv\"\"\"\n",
    "        if metadata_path is None:\n",
    "            data_path = Path(PATHS['extracted_data'])\n",
    "            metadata_path = list(data_path.rglob('*arxiv-metadata*.json'))[0]\n",
    "        \n",
    "        print(f\"Загрузка данных из: {metadata_path}\")\n",
    "        with open(metadata_path, 'r', encoding='utf-8') as f:\n",
    "            metadata = json.load(f)\n",
    "        \n",
    "        self.metadata_df = pd.DataFrame(metadata)\n",
    "        \n",
    "        # Оставляем только нужные поля и очищаем\n",
    "        self.metadata_df = self.metadata_df[['id', 'title', 'abstract']].copy()\n",
    "        self.metadata_df['abstract'] = self.metadata_df['abstract'].fillna('')\n",
    "        self.metadata_df['title'] = self.metadata_df['title'].fillna('')\n",
    "        \n",
    "        print(f\"Загружено статей: {len(self.metadata_df)}\")\n",
    "        return self.metadata_df\n",
    "    \n",
    "    def build_index(self, batch_size: int = 32, save_path: Optional[str] = None):\n",
    "        \"\"\"Построение FAISS индекса\"\"\"\n",
    "        if self.metadata_df is None:\n",
    "            raise ValueError(\"Сначала загрузите данные через load_data()\")\n",
    "        \n",
    "        print(\"Генерация embeddings...\")\n",
    "        embedder = self._get_embedder()\n",
    "        \n",
    "        # Генерируем эмбеддинги батчами\n",
    "        texts = self.metadata_df['abstract'].tolist()\n",
    "        self.embeddings = embedder.encode(\n",
    "            texts,\n",
    "            batch_size=batch_size,\n",
    "            show_progress_bar=True,\n",
    "            convert_to_numpy=True,\n",
    "            normalize_embeddings=True\n",
    "        )\n",
    "        \n",
    "        # Создаем FAISS индекс (Inner Product для нормализованных векторов)\n",
    "        print(\"Построение FAISS индекса...\")\n",
    "        dimension = self.embeddings.shape[1]\n",
    "        self.index = faiss.IndexFlatIP(dimension)\n",
    "        self.index.add(self.embeddings)\n",
    "        \n",
    "        print(f\"Индекс построен: {self.index.ntotal} документов, размерность {dimension}\")\n",
    "        \n",
    "        # Опционально сохраняем\n",
    "        if save_path:\n",
    "            self.save_index(save_path)\n",
    "    \n",
    "    def search(\n",
    "    self, \n",
    "    query: str, \n",
    "    k: int = 10,\n",
    "    rerank_top_k: Optional[int] = None\n",
    ") -> Tuple[List[str], List[float], pd.DataFrame]:\n",
    "    \n",
    "        if self.index is None:\n",
    "            raise ValueError(\"Индекс не построен\")\n",
    "        \n",
    "        # Автоматически увеличиваем кандидатов для reranking\n",
    "        if self.use_reranker and rerank_top_k is None:\n",
    "            rerank_top_k = k * 10  \n",
    "        \n",
    "        # Поиск через FAISS\n",
    "        embedder = self._get_embedder()\n",
    "        query_embedding = embedder.encode([query], convert_to_numpy=True, normalize_embeddings=True)\n",
    "        \n",
    "        search_k = rerank_top_k if rerank_top_k else k\n",
    "        distances, indices = self.index.search(query_embedding, search_k)\n",
    "        \n",
    "        results_df = self.metadata_df.iloc[indices[0]].copy()\n",
    "        results_df['score'] = distances[0]\n",
    "        \n",
    "        # Reranking\n",
    "        if self.use_reranker and rerank_top_k:\n",
    "            reranker = self._get_reranker()\n",
    "            pairs = [[query, doc] for doc in results_df['abstract'].tolist()]\n",
    "            rerank_scores = reranker.predict(pairs)\n",
    "            results_df['rerank_score'] = rerank_scores\n",
    "            results_df = results_df.nlargest(k, 'rerank_score')\n",
    "        else:\n",
    "            results_df = results_df.head(k)\n",
    "        \n",
    "        ids = results_df['id'].tolist()\n",
    "        scores = (results_df['rerank_score'] if 'rerank_score' in results_df.columns \n",
    "                else results_df['score']).tolist()\n",
    "        \n",
    "        return ids, scores, results_df\n",
    "    \n",
    "    def save_index(self, path: str):\n",
    "        \"\"\"Сохранение индекса и метаданных\"\"\"\n",
    "        path = Path(path)\n",
    "        path.mkdir(parents=True, exist_ok=True)\n",
    "        \n",
    "        # Сохраняем FAISS индекс\n",
    "        faiss.write_index(self.index, str(path / 'faiss.index'))\n",
    "        \n",
    "        # Сохраняем embeddings\n",
    "        np.save(path / 'embeddings.npy', self.embeddings)\n",
    "        \n",
    "        # Сохраняем метаданные\n",
    "        self.metadata_df.to_parquet(path / 'metadata.parquet')\n",
    "        \n",
    "        print(f\"Индекс сохранен в: {path}\")\n",
    "    \n",
    "    def load_index(self, path: str):\n",
    "        \"\"\"Загрузка сохраненного индекса\"\"\"\n",
    "        path = Path(path)\n",
    "        \n",
    "        # Загружаем FAISS индекс\n",
    "        self.index = faiss.read_index(str(path / 'faiss.index'))\n",
    "        \n",
    "        # Загружаем embeddings\n",
    "        self.embeddings = np.load(path / 'embeddings.npy')\n",
    "        \n",
    "        # Загружаем метаданные\n",
    "        self.metadata_df = pd.read_parquet(path / 'metadata.parquet')\n",
    "        \n",
    "        print(f\"Индекс загружен: {self.index.ntotal} документов\")\n",
    "\n",
    "# ВСПОМОГАТЕЛЬНЫЕ ФУНКЦИИ ДЛЯ ОЦЕНКИ\n",
    "\n",
    "\n",
    "def calculate_mrr(predictions: List[List[str]], ground_truth: List[str], k: int = 10) -> float:\n",
    "    \"\"\"\n",
    "    Mean Reciprocal Rank\n",
    "    \n",
    "    Args:\n",
    "        predictions: список списков предсказанных id (для каждого запроса)\n",
    "        ground_truth: список правильных id\n",
    "    \"\"\"\n",
    "    reciprocal_ranks = []\n",
    "    \n",
    "    for pred_ids, true_id in zip(predictions, ground_truth):\n",
    "        if true_id in pred_ids[:k]:\n",
    "            rank = pred_ids.index(true_id) + 1\n",
    "            reciprocal_ranks.append(1 / rank)\n",
    "        else:\n",
    "            reciprocal_ranks.append(0)\n",
    "    \n",
    "    return sum(reciprocal_ranks) / len(reciprocal_ranks)\n",
    "\n",
    "\n",
    "def evaluate_retrieval(rag: ArxivRAG, test_df: pd.DataFrame, k: int = 10) -> Dict:\n",
    "    \"\"\"Оценка качества retrieval системы\"\"\"\n",
    "    import time\n",
    "    \n",
    "    predictions = []\n",
    "    times = {'embedding': 0, 'search': 0, 'rerank': 0, 'total': 0}\n",
    "    \n",
    "    print(f\"Оценка на {len(test_df)} запросах...\")\n",
    "    \n",
    "    for idx, row in test_df.iterrows():\n",
    "        start_time = time.time()\n",
    "        \n",
    "        query = row['query']\n",
    "        ids, scores, _ = rag.search(query, k=k)\n",
    "        predictions.append(ids)\n",
    "        \n",
    "        times['total'] += time.time() - start_time\n",
    "        \n",
    "        if (idx + 1) % 100 == 0:\n",
    "            print(f\"Обработано: {idx + 1}/{len(test_df)}\")\n",
    "    \n",
    "    # Вычисляем метрики\n",
    "    mrr = calculate_mrr(predictions, test_df['id'].tolist(), k=k)\n",
    "    \n",
    "    results = {\n",
    "        'mrr': mrr,\n",
    "        'avg_time_ms': times['total'] / len(test_df) * 1000,\n",
    "        'total_time': times['total'],\n",
    "        'predictions': predictions\n",
    "    }\n",
    "    \n",
    "    print(f\"\\n{'='*60}\")\n",
    "    print(f\"MRR@{k}: {mrr:.4f}\")\n",
    "    print(f\"Среднее время на запрос: {results['avg_time_ms']:.2f} ms\")\n",
    "    print(f\"Общее время: {results['total_time']:.2f} s\")\n",
    "    print(f\"{'='*60}\")\n",
    "    \n",
    "    return results"
   ]
  },
